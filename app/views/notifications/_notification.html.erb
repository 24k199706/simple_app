<%= @notifications.each do|infomation| %>
  <% if infomation.action=="like"%>
  <table>
      <%= link_to notification_form(infomation), posts_show_path(infomation.post_id) %><br/>
    <% else %>
      <%= link_to notification_form(infomation), posts_show_path(infomation.post_id) %>
  <% end %>
<% end %>  
