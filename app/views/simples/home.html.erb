
<!--ログインしているときの処理 -->
<% if logged_in? %>
    <table class="table table-hover">
        <tbody>
            <% @post.each do |post| %>
                <tr>
                <td><%= current_user.name %>
                <%= link_to post.title , posts_show_path(post) %><br>
                <%= post.content %></td>
                </tr>
            <% end %>
        </tbody>
    </table>
<% else %>
    ホーム画面
<% end %>